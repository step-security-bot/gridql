classDiagram

    Farm *-- Coop
    class Farm {
        name: String!

        getByName(name: String): Farm
    }


    Coop *-- Hen
    class Coop {
        name: String! [faker: "location.country"]
        farm_id: Farm!

        getByName(name: String): Coop
    }

    class Hen {
        name: String! [faker: "person.firstName"]
        coop_id: Coop
        eggs: Int [min: 0]
        dob: Date

        getByName(name: String): [Hen]
    }
